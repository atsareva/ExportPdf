<config>
    <modules>
        <Tsareva_ExportPdf>
            <version>0.1.0</version>
        </Tsareva_ExportPdf>
    </modules>
    <global>
        <helpers>
            <tsareva_exportpdf>
                <class>Tsareva_ExportPdf_Helper</class>
            </tsareva_exportpdf>
        </helpers>
        <blocks>
            <tsareva_exportpdf>
                <class>Tsareva_ExportPdf_Block</class>
            </tsareva_exportpdf>
            <adminhtml>
                <rewrite>
                    <sales_order_grid>Tsareva_ExportPdf_Block_Adminhtml_Sales_Order_Grid</sales_order_grid>
                </rewrite>
            </adminhtml>
        </blocks>
        <models>
            <tsareva_exportpdf>
                <class>Tsareva_ExportPdf_Model</class>
            </tsareva_exportpdf>
        </models>
    </global>
    <frontend>
        <routers>
            <tsareva_exportpdf>
                <use>standard</use>
                <args>
                    <module>Tsareva_ExportPdf</module>
                    <frontName>exportPdf</frontName>
                </args>
            </tsareva_exportpdf>
        </routers>
    </frontend>
</config>

